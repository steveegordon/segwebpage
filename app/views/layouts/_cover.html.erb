<div class="fh5co-cover fh5co-cover-style-2 js-full-height <%= cover(yield(:title)) %>" data-stellar-background-ratio="0.5" data-next="yes"
        <span class="scroll-btn wow fadeInUp" data-wow-duration="1s" data-wow-delay="1.4s">
        <a href="#">
          <span class="mouse"><span></span></span>
        </a>
      </span>
       <%= render 'layouts/header' %>
      <div class="fh5co-overlay"></div>
      <div class="fh5co-cover-text">
        <div class="container">
          <div class="row">
            <div class="col-md-push-6 col-md-6 full-height js-full-height">
              <div class="fh5co-cover-intro">
                <h1 class="cover-text-lead wow fadeInUp" data-wow-duration="1s" data-wow-delay=".5s">Steve E. Gordon <span class="subhead"></br>Web Developer</span> </h1>
                <h2 class="cover-text-sublead wow fadeInUp" data-wow-duration="1s" data-wow-delay=".8s">Welcome to my personal webpage. Here you can find out who I am, look at some of my projects, and find out what i'm learning/reading about.</h2>
                <p class="wow fadeInUp" data-wow-duration="1s" data-wow-delay="1.1s">
                <%= link_to 'See My Work', projects_path , class: "btn btn-primary btn-outline btn-lg"%>
              </div>
            </div>
          </div>
        </div>  
      </div>
    </div>